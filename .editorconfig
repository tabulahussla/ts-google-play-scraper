# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
indent_style = tab
indent_size = 4
charset = utf-8
end_of_line = lf
insert_final_newline = true

# Unix-style newlines with a newline ending every file
[{*.yml,*.yaml}]
indent_style = space
indent_size = 2
